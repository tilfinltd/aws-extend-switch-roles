{
  "version": "0.1.0",
  "name": "Quick open AWS",
  "description": "Quick opent AWS",
  "short_name": "Extend AESR",
  "permissions": ["activeTab", "storage"],
  "icons" : {
    "48": "icons/Icon_48x48_g.png",
    "128": "icons/golden_icon.png"
  },
  "browser_action": {
    "default_title": "Quick Open AWS",
    "default_icon": "icons/Icon_38x38.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.console.aws.amazon.com/*",
        "https://phd.aws.amazon.com/*",
        "https://*.console.amazonaws-us-gov.com/*",
        "https://*.console.amazonaws.cn/*"
      ],
      "all_frames": true,
      "js": [
        "js/content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    "js/attach_target.js"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"
      }
    }
  },
  "manifest_version": 2
}
